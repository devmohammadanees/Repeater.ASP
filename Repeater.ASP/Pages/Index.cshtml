@page
@model IndexModel
@{
  ViewData["Title"] = "Home page";
}

<div class="container-fluid mt-3 mb-3">
  <div class="row">
    <div class="col">
      <h2>Australian Homes</h2>
    </div>
  </div>
  <form method="POST">
    @Html.EditorFor(m => m.AustralianHomes)
    <button  type="submit" class="btn btn-primary">Submit</button>
  </form>
</div>

<div style="display: none">
  @Html.EditorFor(m => m.EmptyState)
  @Html.EditorFor(m => m.EmptyLocation)
  @Html.EditorFor(m => m.EmptyStreet)
  @Html.EditorFor(m => m.EmptyHome)
  @Html.EditorFor(m => m.EmptyRoom)
</div>
