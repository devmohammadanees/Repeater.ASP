@model Models.State

<div class="form-group">
    <label asp-for="Name" class="control-label"></label>
    <input asp-for="Name" class="form-control" />
    <span asp-validation-for="Name" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="IsTerritory" class="control-label"></label>
    <input asp-for="IsTerritory" class="form-control" />
    <span asp-validation-for="IsTerritory" class="text-danger"></span>
</div>
@foreach (var location in Model.Locations)
{
    @await Html.PartialAsync("Location", location)
}
